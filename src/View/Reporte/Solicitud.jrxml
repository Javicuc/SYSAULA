     `Aula` Aula INNER JOIN `Solicitud` Solicitud ON Aula.`ID_Aula` = Solicitud.`FK_Aula`
     INNER JOIN `Edificio` Edificio ON Aula.`FK_Edificio` = Edificio.`ID_Nombre`
